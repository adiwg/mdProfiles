  "homepage": "https://github.com/adiwg/mdProfile#readme",
  "devDependencies": {
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-node": "^10.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "gh-pages": "^2.1.1",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.2",
    "mocha": "^6.2.1",
    "prettier": "^1.18.2",
    "release-it": "^12.4.3",
    "replace-in-file": "^4.1.3",
